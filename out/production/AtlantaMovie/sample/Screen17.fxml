<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox fx:controller="sample.Screen17Controller" prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="265.0" prefWidth="640.0" VBox.vgrow="ALWAYS">
            <children>
                <Text layoutX="246.0" layoutY="41.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Create Movie">
                    <font>
                        <Font name="System Bold" size="25.0" />
                    </font>
                </Text>
                <Label layoutX="95.0" layoutY="76.0" text="Name">
                    <font>
                        <Font size="15.0" />
                    </font>
                </Label>
                <TextField fx:id = "movieNameField" layoutX="145.0" layoutY="74.0" />
                <Label layoutX="354.0" layoutY="76.0" text="Duration">
                    <font>
                        <Font size="15.0" />
                    </font>
                </Label>
                <TextField fx:id = "movieDurationField" layoutX="424.0" layoutY="74.0" />
                <Label  layoutX="266.0" layoutY="139.0" text="Release Date">
                    <font>
                        <Font size="15.0" />
                    </font>
                </Label>
                <Button  fx:id="backButton" layoutX="95.0" layoutY="208.0" mnemonicParsing="false" prefHeight="33.0" prefWidth="86.0" text="Back" onAction="#backToAdminFunc"/>
                <Button layoutX="472.0" layoutY="208.0" mnemonicParsing="false" prefHeight="34.0" prefWidth="86.0" text="Create" onAction="#createMovie"/>
                <DatePicker fx:id = "movieReleaseDatePicker" layoutX="365.0" layoutY="136.0" prefHeight="26.0" prefWidth="156.0" onAction="#extractDate"/>
            </children>
        </AnchorPane>
    </children>
</VBox>